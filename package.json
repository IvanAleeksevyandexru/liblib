{
  "name": "epgu-lib",
  "version": "0.1.124-ivy",
  "publishConfig": {
    "registry": "http://nexus.gosuslugi.local/content/repositories/npm-internal/",
    "access": "public"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "future-lib-release": "CURRENT_BRANCH=$(git rev-parse --abbrev-ref HEAD | sed s/branch-prod-fed-//) && A=$(echo $CURRENT_BRANCH | cut -d'.' -f1) && B=$(echo $CURRENT_BRANCH | cut -d'.' -f2) && C=$(echo $CURRENT_BRANCH | cut -d'.' -f3) && D=$(echo $CURRENT_BRANCH | cut -d'.' -f4) && LIB_VERSION=${A}.${B}.${C}-${D} && CURRENT_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g') && echo $CURRENT_VERSION && echo $LIB_VERSION && if [ $CURRENT_VERSION != $LIB_VERSION ]                     \nthen\n        npm version $LIB_VERSION\nfi\n",
    "postlib-release": "cd ./dist/ui && npm publish",
    "lib-watch": "cross-env NODE_OPTIONS=--max_old_space_size=5110 ng build ui --watch",
    "lib-build:prod": "cross-env NODE_OPTIONS=--max_old_space_size=5110 ng build ui && npm run cp-lib-assets",
    "lib-release": "npm run lib-build:prod",
    "cp-lib-assets": "cross-env cpx \"./projects/assets/**/*\" \"./dist/ui/assets\"",
    "publish": "cd dist/ui/ && npm publish"
  },
